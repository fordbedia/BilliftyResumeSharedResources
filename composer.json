{
  "name": "billiftyresume/shared-resources",
  "description": "Shared Laravel module with modular support for User, Post, Comments, etc.",
  "type": "library",
  "version": "1.0.0",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "BilliftyResumeSDK\\SharedResources\\": "src/",
       "BilliftyResumeSDK\\SharedResources\\TestCase\\": "TestCase/"
    },
    "files": [
      "src/Modules/Builder/Support/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "BilliftyResumeSDK\\SharedResources\\SharedResourceServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0",
    "laravel/passport": "^12.4"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
